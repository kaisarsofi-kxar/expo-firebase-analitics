{
    "cli": {
      "version": ">= 14.4.1",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        }
      },
      "preview": {
        "distribution": "internal",
        "environment": "preview",
        "channel": "staging",
        "android": {
          "buildType": "apk"
        }
      },
      "production": {
        "autoIncrement": true,
        "environment": "production",
        "android": {
          "buildType": "apk"
        }
      }
    },
    "submit": {
      "production": {}
    }
  }
  